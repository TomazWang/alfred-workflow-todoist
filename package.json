{
  "name": "alfred-workflow-todoist",
  "version": "5.8.4",
  "description": "",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/moranje/alfred-workflow-todoist.git"
  },
  "license": "MIT",
  "author": {
    "name": "M. Oranje",
    "email": "martieno@gmail.com"
  },
  "files": [
    "dist"
  ],
  "main": "dist/workflow/alfred-workflow-todoist.js",
  "typings": "src/typings.d.ts",
  "scripts": {
    "prebuild": "ts-node tools/move-files.ts moveToTemp && rimraf dist",
    "build": "NODE_ENV=production ts-node tools/build.ts",
    "build:all": "npm run build:grammar && npm run build && npm run build:workflow",
    "build:dev": "NODE_ENV=development ts-node tools/build.ts",
    "build:grammar": "npx nearleyc src/lib/todoist/grammar.ne -o src/lib/todoist/grammar.ts && echo \"// @ts-nocheck\n$(cat src/lib/todoist/grammar.ts)\" > src/lib/todoist/grammar.ts",
    "build:workflow": "ts-node tools/build-workflow.ts",
    "bump-version": "ts-node tools/bump-version.ts",
    "commit": "./node_modules/cz-customizable/standalone.js",
    "debug": "ts-node tools/debug.ts",
    "lint": "npx prettier --find-config-path --loglevel silent --write \"{src,test,tools}/**/*.{ts,js}\" && npx eslint src/**/*.ts --fix",
    "prepush": "npm run build && npm run test:prod",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "restore": "mkdir -p dist/workflow && npm run postbuild",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "setup:dev": "ts-node tools/setup.ts",
    "start": "rollup -c rollup.config.ts -w",
    "test": "npm run lint && jest",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --no-cache --watch --runInBand",
    "test:prod": "NODE_ENV=production npm run test -- --coverage --no-cache",
    "test:ui": "npx majestic",
    "test:watch": "jest --watch"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./cz-config.js"
    }
  },
  "dependencies": {
    "@types/fuzzy-search": "2.1.0",
    "ajv": "6.10.2",
    "babel-polyfill": "6.26.0",
    "cache-conf": "0.6.0",
    "conf": "6.2.0",
    "date-fns": "2.8.1",
    "fast-plist": "0.1.2",
    "fuzzy-search": "3.0.2",
    "got": "10.0.1",
    "lodash.find": "4.6.0",
    "lodash.omit": "4.5.0",
    "lodash.remove": "4.7.0",
    "lodash.unionby": "4.8.0",
    "loud-rejection": "2.2.0",
    "lru-cache": "5.1.1",
    "macos-version": "5.2.0",
    "md5": "2.2.1",
    "mkdirp": "0.5.1",
    "moo": "0.5.1",
    "nearley": "2.19.0",
    "node-notifier": "5.4.0",
    "stampit": "4.3.0",
    "todoist-rest-api": "1.2.5",
    "write-json-file": "4.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-syntax-top-level-await": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@eclass/semantic-release-sentry-releases": "^1.0.0",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-json": "^4.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.3.0",
    "@semantic-release/changelog": "5.0.0",
    "@semantic-release/exec": "5.0.0",
    "@semantic-release/git": "9.0.0",
    "@types/chance": "^1.0.8",
    "@types/conf": "3.0.0",
    "@types/inquirer": "6.5.0",
    "@types/jest": "25.1.1",
    "@types/md5": "2.1.33",
    "@types/mkdirp": "0.5.2",
    "@types/moo": "0.5.1",
    "@types/nearley": "2.11.0",
    "@types/nock": "11.1.0",
    "@types/node": "13.5.3",
    "@types/shelljs": "0.8.6",
    "@types/write-json-file": "3.2.1",
    "@typescript-eslint/eslint-plugin": "2.18.0",
    "@typescript-eslint/parser": "2.18.0",
    "@wessberg/rollup-plugin-ts": "^1.2.15",
    "babel-jest": "^25.1.0",
    "bestzip": "2.1.5",
    "colors": "1.4.0",
    "coveralls": "3.0.9",
    "cz-customizable": "6.2.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-jsdoc": "^21.0.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-simple-import-sort": "^5.0.1",
    "husky": "4.2.1",
    "inquirer": "7.0.4",
    "jest": "25.1.0",
    "jest-mock-process": "^1.3.0",
    "lint-staged": "10.0.6",
    "mkdirp": "1.0.3",
    "nock": "11.7.2",
    "prettier": "1.19.1",
    "rollup": "1.30.1",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-node-externals": "^2.1.3",
    "rollup-plugin-sourcemaps": "^0.5.0",
    "rollup-plugin-terser": "^5.2.0",
    "semantic-release": "17.0.1",
    "shelljs": "0.8.3",
    "ts-node": "8.6.2",
    "tslib": "1.10.0",
    "typescript": "3.7.5"
  },
  "engines": {
    "node": ">=10"
  }
}
