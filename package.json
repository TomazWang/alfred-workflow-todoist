    "jest": "25.1.0",
    "jest-mock-process": "^1.3.2",
    "json-schema-typed": "^7.0.3",
    "lint-staged": "10.0.7",
    "markdownlint-cli": "^0.22.0",
    "mkdirp": "1.0.3",
    "nock": "11.7.2",
    "npm-run-all": "^4.1.5",
    "prettier": "1.19.1",
    "rollup": "1.31.0",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-sourcemaps": "^0.5.0",
    "rollup-plugin-terser": "^5.2.0",
