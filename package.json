{
  "name": "alfred-workflow-todoist",
  "version": "5.8.4",
  "description": "",
  "keywords": [],
  "main": "dist/workflow/alfred-workflow-todoist.js",
  "typings": "src/typings.d.ts",
  "files": [
    "dist"
  ],
  "author": {
    "name": "M. Oranje",
    "email": "martieno@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moranje/alfred-workflow-todoist.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "setup:dev": "ts-node tools/setup.ts",
    "debug": "ts-node tools/debug.ts",
    "lint": "npx prettier --find-config-path --loglevel silent --write \"{src,test,tools}/**/*.{ts,js}\" && npx eslint src/**/*.ts --fix",
    "prebuild": "ts-node tools/move-files.ts moveToTemp && rimraf dist",
    "build": "ts-node tools/build.ts",
    "build:grammar": "npx nearleyc src/todoist/grammar.ne -o src/todoist/grammar.ts",
    "build:workflow": "ts-node tools/build-workflow.ts",
    "restore": "mkdir -p dist/workflow && npm run postbuild",
    "start": "rollup -c rollup.config.ts -w",
    "test": "npm run lint && jest",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --no-cache --watch --runInBand",
    "test:watch": "jest --watch --silent",
    "test:prod": "npm run test -- --coverage --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "./node_modules/cz-customizable/standalone.js",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "bump-version": "ts-node tools/bump-version.ts",
    "prepush": "npm run build && npm run test:prod"
  },
  "dependencies": {
    "@types/fuzzy-search": "2.1.0",
    "ajv": "6.10.2",
    "babel-polyfill": "6.26.0",
    "cache-conf": "0.6.0",
    "conf": "6.2.0",
    "date-fns": "2.8.1",
    "fast-plist": "0.1.2",
    "fuzzy-search": "3.0.2",
    "got": "10.0.1",
    "lodash.find": "4.6.0",
    "lodash.omit": "4.5.0",
    "lodash.remove": "4.7.0",
    "lodash.unionby": "4.8.0",
    "loud-rejection": "2.2.0",
    "lru-cache": "5.1.1",
    "macos-version": "5.2.0",
    "md5": "2.2.1",
    "mkdirp": "0.5.1",
    "moo": "0.5.1",
    "nearley": "2.19.0",
    "node-notifier": "5.4.0",
    "stampit": "4.3.0",
    "todoist-rest-api": "1.2.5",
    "write-json-file": "4.2.1"
  },
  "devDependencies": {
    "@babel/polyfill": "7.7.0",
    "@semantic-release/changelog": "3.0.6",
    "@semantic-release/exec": "3.3.8",
    "@semantic-release/git": "7.0.18",
    "@types/conf": "3.0.0",
    "@types/inquirer": "6.5.0",
    "@types/jest": "24.0.23",
    "@types/lodash.find": "4.6.6",
    "@types/lodash.omit": "4.5.6",
    "@types/lodash.remove": "4.7.6",
    "@types/lodash.unionby": "4.8.6",
    "@types/lru-cache": "5.1.0",
    "@types/md5": "2.1.33",
    "@types/mkdirp": "0.5.2",
    "@types/moo": "0.5.1",
    "@types/nearley": "2.11.0",
    "@types/nock": "11.1.0",
    "@types/node": "12.12.14",
    "@types/shelljs": "0.8.6",
    "@types/write-json-file": "3.2.1",
    "@typescript-eslint/eslint-plugin": "2.9.0",
    "@typescript-eslint/parser": "2.9.0",
    "bestzip": "2.1.5",
    "colors": "1.4.0",
    "coveralls": "3.0.9",
    "cz-customizable": "6.2.0",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-prettier": "3.1.1",
    "husky": "3.1.0",
    "inquirer": "7.0.0",
    "jest": "24.9.0",
    "lint-staged": "9.5.0",
    "nock": "11.7.0",
    "prettier": "1.19.1",
    "rollup": "1.27.6",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "0.25.2",
    "semantic-release": "15.13.31",
    "shelljs": "0.8.3",
    "ts-jest": "24.2.0",
    "ts-node": "8.5.4",
    "tslib": "1.10.0",
    "typedoc": "0.15.3",
    "typescript": "3.7.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./cz-config.js"
    }
  }
}
