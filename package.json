{
  "name": "alfred-workflow-todoist",
  "version": "6.0.0-alpha.6",
  "description": "",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/moranje/alfred-workflow-todoist.git"
  },
  "license": "MIT",
  "author": {
    "name": "M. Oranje",
    "email": "martieno@gmail.com"
  },
  "files": [
    "dist"
  ],
  "main": "dist/workflow/alfred-workflow-todoist.js",
  "typings": "src/typings.d.ts",
  "scripts": {
    "prebuild": "ts-node tools/move-files.ts storeAssets && rimraf dist",
    "build": "ts-node tools/build.ts production",
    "build:all": "run-s build:grammar build build:workflow",
    "build:dev": "ts-node tools/move-files.ts storeAssets && ts-node tools/build.ts",
    "build:grammar": "npx nearleyc src/lib/todoist/grammar.ne -o src/lib/todoist/grammar.ts && echo \"// @ts-nocheck\n$(cat src/lib/todoist/grammar.ts)\" > src/lib/todoist/grammar.ts",
    "build:workflow": "ts-node tools/build-workflow.ts",
    "bump-version": "ts-node tools/bump-version.ts",
    "commit": "./node_modules/cz-customizable/standalone.js",
    "debug": "ts-node tools/debug.ts",
    "lint:eslint": "eslint --fix",
    "lint:markdownlint": "markdownlint --fix",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:all:eslint": "npm run lint:eslint -- \"src/**/*.ts\"",
    "lint:all:markdownlint": "npm run lint:markdownlint -- \"*.md\"",
    "lint:all:prettier": "npm run lint:prettier -- \"{src,tools}/**/*.{ts,js}\"",
    "lint": "run-s lint:all:*",
    "prepush": "npm run build && npm run test:prod",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "semantic-release": "semantic-release",
    "setup:dev": "ts-node tools/setup.ts",
    "start": "rollup -c rollup.config.ts -w",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --no-cache --watch --runInBand",
    "test:ui": "npx majestic",
    "test:unit": "jest",
    "test:unit:file": "npm run test:unit -- --findRelatedTests --passWithNoTests",
    "test:watch": "jest --watch",
    "test:unit:watch": "npm run test:unit -- --watch --notify --notifyMode change",
    "test:unit:ci": "npm run test:unit -- --coverage --ci",
    "test": "NODE_ENV=production run-s lint test:unit:ci"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./cz-config.js"
    }
  },
  "dependencies": {
    "@sentry/integrations": "^5.12.0",
    "@sentry/node": "^5.12.3",
    "clean-stack": "2.2.0",
    "compare-versions": "^3.5.1",
    "conf": "6.2.0",
    "core-js": "^3.6.4",
    "date-fns": "2.9.0",
    "deep-equal": "^2.0.1",
    "dotenv": "^8.2.0",
    "fuzzy-search": "3.1.0",
    "got": "10.5.2",
    "macos-version": "5.2.0",
    "moo": "0.5.1",
    "nearley": "2.19.1",
    "new-github-issue-url": "^0.2.1",
    "open": "^7.0.2",
    "todoist-rest-api": "1.3.2",
    "write-json-file": "4.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-syntax-top-level-await": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@eclass/semantic-release-sentry-releases": "^2.1.0",
